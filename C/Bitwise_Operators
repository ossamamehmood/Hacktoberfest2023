#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <stdio.h>

void calculate_the_maximum(int n, int k) {
    int maxAnd = 0, maxOr = 0, maxXor = 0;

    for (int a = 1; a <= n; a++) {
        for (int b = a + 1; b <= n; b++) {
            int currentAnd = a & b;
            int currentOr = a | b;
            int currentXor = a ^ b;

            if (currentAnd < k && currentAnd > maxAnd) {
                maxAnd = currentAnd;
            }
            if (currentOr < k && currentOr > maxOr) {
                maxOr = currentOr;
            }
            if (currentXor < k && currentXor > maxXor) {
                maxXor = currentXor;
            }
        }
    }

    printf("%d\n%d\n%d\n", maxAnd, maxOr, maxXor);
}


int main() {
    int n, k;
  
    scanf("%d %d", &n, &k);
    calculate_the_maximum(n, k);
 
    return 0;
}
