{
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "Domain": "{domain}",
    "TenantId": "{tenantid}",
    "ClientId": "{clientid}",
    "CallbackPath": "/signin-oidc"
  },
  "SharePoint": {
    "Url": "{sharepoint site url}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "MicrosoftGraph": {
    "BaseUrl": "https://graph.microsoft.com/v1.0",
    "Scopes": "user.read"
  }
}